http:
  routers:
    factorio:
      rule: Host(`{{ factorio_subdomain }}.{{ domain_name }}`) || Host(`{{ factorio_subdomain }}.{{ local_domain_name }}`)
      service: factorio
      entryPoints:
        - websecure
      tls:
        certResolver: cloudflare
  services:
    factorio:
      loadBalancer:
        servers:
          - url: http://192.168.1.205:8888
