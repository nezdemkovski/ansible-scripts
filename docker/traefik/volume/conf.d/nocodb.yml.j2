http:
  routers:
    nocodb:
      rule: Host(`{{ nocodb_subdomain }}.{{ domain_name }}`) || Host(`{{ nocodb_subdomain }}.{{ local_domain_name }}`)
      service: nocodb
      entryPoints:
        - websecure
      tls:
        certResolver: cloudflare
  services:
    nocodb:
      loadBalancer:
        servers:
          - url: http://192.168.1.143:8080
