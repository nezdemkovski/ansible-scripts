http:
  routers:
    excalidraw:
      rule: Host(`{{ excalidraw_subdomain }}.{{ domain_name }}`) || Host(`{{ excalidraw_subdomain }}.{{ local_domain_name }}`)
      service: excalidraw
      entryPoints:
        - websecure
      tls:
        certResolver: cloudflare
  services:
    excalidraw:
      loadBalancer:
        servers:
          - url: http://192.168.1.204:3000
